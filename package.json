{
  "name": "tapi-ts-poc",
  "version": "1.0.0",
  "description": "tapi ts poc",
  "scripts": {
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "prod": "npm prune --production && npm install --production",
    "start-sls": "tsc && node node_modules/.bin/serverless offline start -r us-east-1 --noTimeout --skipCacheInvalidation",
    "debug-sls": "tsc && node --inspect-brk=9229 node_modules/.bin/serverless offline start -r us-east-1 --noTimeout --skipCacheInvalidation",
    "bundle-dependencies": "bundle-dependencies",
    "pack2zip": "npm pack; tar2zip *.tgz; del-cli '*.tgz'",
    "build": "del-cli '*.zip'; npm run bundle-dependencies update && npm run prod && npm run pack2zip && npm install"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/websockets": "^4.6.6",
    "typescript": "^2.6.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.2.0",
    "bunyan": "^1.8.12",
    "mysql": "^2.15.0",
    "safe-json-stringify": "^1.1.0",
    "moment": "^2.22.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.0.1",
    "@types/bunyan": "^1.8.4",
    "@types/mysql": "^2.15.4",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "serverless": "^1.26.1",
    "serverless-offline": "^3.20.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "bundle-dependencies": "^1.0.2",
    "tar-to-zip": "^1.3.4",
    "del-cli": "^1.0.0"
  },
  "bundledDependencies": [
    "@nestjs/common",
    "@nestjs/core",
    "@nestjs/microservices",
    "@nestjs/testing",
    "@nestjs/websockets",
    "aws-lambda",
    "aws-serverless-express",
    "bunyan",
    "mysql",
    "reflect-metadata",
    "rxjs",
    "safe-json-stringify",
    "typescript"
  ]
}
